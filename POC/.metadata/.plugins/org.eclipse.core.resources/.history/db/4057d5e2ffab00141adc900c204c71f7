package com.market;

/**
 * The SupermarketClient program is consumer of SupermarketRegister web-service
 *
 * @author Ashish Sharma
 * @version 1.0
 * @since 2015-02-03
 */
public class SupermarketClient {

	/**
	   * This is the main method which is used as SupermarketClient to access SupermarketRegister web-service
	   * @param args Unused.
	   * @return Nothing.
	   * @exception IOException On input error.
	   * @see IOException
	   */
	public static void main(String[] args) {
		Supermarket supermarket = new SupermarketRegister();
		System.out.println(supermarket.checkout("ABBACBBAB"));	
	}

}
